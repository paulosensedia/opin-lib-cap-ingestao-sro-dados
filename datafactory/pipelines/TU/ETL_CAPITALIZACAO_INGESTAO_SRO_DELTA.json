                                {
                                    "activity": "SET LAST_REQUEST_ID",
                                    "dependencyConditions": [
                                        "Succeeded"
                                    ]
                                }
                            ],
                            "userProperties": [],
                            "typeProperties": {
                                "expression": {
                                    "value": "@not(equals(variables('last_request_id'), 'None'))",
                                    "type": "Expression"
                                },
                                "ifFalseActivities": [
                                    {
                                        "name": "GET OPENHUB_FIRST",
                                        "type": "Copy",
                                        "dependsOn": [],
