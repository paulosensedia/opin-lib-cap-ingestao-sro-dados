                ]
            },
            {
                "name": "FILE_VALIDATOR",
                "type": "Validation",
                "dependsOn": [],
                "userProperties": [],
                "typeProperties": {
                    "dataset": {
                        "referenceName": "DATASET_FSERVER_CAP_INGESTAO_SRO",
                        "type": "DatasetReference",
                        "parameters": {
