                        "value": "@activity('LOOKUP_TABLES').output.value",
                        "type": "Expression"
                    },
                    "isSequential": true,
                    "activities": [
                        {
                            "name": "SET LAST_REQUEST_ID",
                            "type": "SetVariable",
                            "dependsOn": [
                                {
